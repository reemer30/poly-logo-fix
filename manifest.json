{
  "name": "PortalLogoFix",
  "version": "1.0",
  "description": "Replaces the 'improved' Cal Poly logo with the old one on the portal.",
  "manifest_version": 2,
  "content_scripts": [{
      "matches": ["*://polylearn.calpoly.edu/*", "*://myportal.calpoly.edu/*"],
      "all_frames": true,
      "js":      ["js/replace_header.js"]
  }],

  "icons": {
    "128": "icons/hand128.png"
  }
}